{
  "name": "platform-01",
  "version": "0.0.4",
  "description": "uf3.0前端框架",
  "author": "socrates",
  "private": false,
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "dev": "cross-env mode=development webpack-dev-server --progress --config config/webpack.config.js",
    "build": "cross-env mode=platform webpack --progress --config config/webpack.config.js",
    "child": "cross-env mode=child node ./config/webpack.run.js",
    "dll": "cross-env mode=dll webpack --progress --config config/webpack.config.js",
    "build-lib": "webpack --config build/webpack.library.js",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "uf-cli": "node ./build/toolbox/cmd.js",
    "storybook": "node ./build/copyStoryStyle && start-storybook -p 8092",
    "gen-docs": "node ./build/toolbox/propsToDoc ",
    "deploy-docs": "node ./build/toolbox/styleToDoc && node ./build/toolbox/propsToDoc && node ./build/copyStoryStyle &&  node --max-old-space-size=4096 ./node_modules/@storybook/vue/bin/build.js  && node ./build/storybook && python ./storybook.py",
    "deploy3-docs": "node ./build/toolbox/styleToDoc && node ./build/toolbox/propsToDoc && node ./build/copyStoryStyle &&  node --max-old-space-size=4096 ./node_modules/@storybook/vue/bin/build.js  && node ./build/storybook && python3 ./storybook.py",
    "deploy": "node ./build/deploy/index.js",
    "start:dev": "cross-env mode=development webpack-dev-server --progress --config config/webpack.config.js",
    "build:dll": "cross-env mode=dll webpack --progress --config config/webpack.config.js",
    "build:frame": "cross-env mode=platform webpack --progress --config config/webpack.config.js",
    "build:child": "cross-env mode=child node ./config/webpack.run.js",
    "update:child": "node utils/update.js",
    "test:run": "cross-env mode=child node ./config/webpack.run.js"
  },
  "lint-staged": {
    "src/**/*.{js,vue}": [
      "./node_modules/.bin/eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "moment": "^2.24.0"
  },
  "gitHead": "f67737ce12c793e74430cface8cfa2cac0d08982"
}
